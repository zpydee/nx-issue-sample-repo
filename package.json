{
  "name": "collabli",
  "engines": {
    "node": "16"
  },
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "lint": "nx lint",
    "build": "nx build",
    "preanalyze": "nx run collabli-pwa-v0:build:production --stats-json",
    "analyze": "webpack-bundle-analyzer dist/collabli-pwa-v0/browser/stats.json -m static -r ./bundle-report/report.html -p 8080",
    "test": "nx test",
    "deploy": "nx deploy",
    "deploy:site": "nx deploy collabli-site",
    "drop:db:dev": "firebase use dev && firebase firestore:delete --all-collections --recursive",
    "emulator:functions": "firebase emulators:start --only functions --inspect-functions",
    "lighthouse": "lighthouse https://next.collabli.app --output-path lighthouse/report.html",
    "lighthouse:report": "npm run lighthouse && open -a \"Google Chrome\" lighthouse/report.html",
    "-": "-----------------------------------------------------------------------------------------------------------------------",
    "// DEV": "",
    "start:dev": "nx run collabli-pwa-v0:serve:development",
    "build:dev": "nx run collabli-pwa-v0:build:development",
    "// STAGING": "",
    "start:beta": "nx run collabli-pwa-v0:serve:beta",
    "build:beta": "nx run collabli-pwa-v0:build:beta",
    "--": "-----------------------------------------------------------------------------------------------------------------------"
  },
  "private": true,
